-title>\n            <v-card-text>\n                <v-form @submit.prevent=\"addDepartment\">\n                    <v-container>\n                        <v-row>\n                            <v-col\n                                cols=\"12\"\n                            >\n                                <v-text-field\n                                label=\"Afdelingnaam*\"\n                                v-model=\"name\"\n                                prepend-icon=\"mdi-office-building\"\n                                required\n                                ></v-text-field>\n                            </v-col>\n\n                            <v-col cols=\"12\">\n                                <v-text-field\n                                label=\"Locatie*\"\n                                v-model=\"location\"\n                                prepend-icon=\"mdi-map-marker\"\n                                required\n                                ></v-text-field>\n                            </v-col>\n\n                            <v-col cols=\"12\">\n                                <v-text-field\n                                label=\"Telefoonnummer*\"\n                                v-model=\"phonenmbr\"\n                                prepend-icon=\"mdi-phone\"\n                                ></v-text-field>\n                            </v-col>\n\n                            <v-col cols=\"12\">\n                                <v-text-field\n                                label=\"Hoofdbegeleider*\"\n                                v-model=\"headattendant\"\n                                prepend-icon=\"mdi-account\"\n                                required\n                                ></v-text-field>\n                            </v-col>\n                        </v-row>\n                    </v-container>\n                    <small>*indicates required field</small>\n                    <v-btn\n                        color=\"red\"\n                        text\n                        @click=\"close\"\n                    >\n                        <v-icon>mdi-delete</v-icon>\n                        Close\n                    </v-btn>\n                    <v-btn\n                        color=\"green\"\n                        text\n                        type=\"submit\"\n                    >\n                        <v-icon>mdi-content-save</v-icon>\n                        Opslaan\n                    </v-btn>\n                </v-form>\n            </v-card-text>\n        </v-card>\n    </v-dialog>\n</template>\n\n<script>\nimport { db } from '@/main';\n  export default {\n    data() {\n        return {\n            dialog: false,\n            name: '',\n            location: '',\n            phonenmbr: '',\n            headattendant: '',\n        }\n    },\n    mounted() {\n        this.getDepartment();\n    },\n    methods: {\n        async getDepartment(){\n            let snapshot = await db.collection(\"afdelingDB\").get();\n            let departments = [];\n            snapshot.forEach(doc => {\n            let appData = doc.data();\n            appData.id = doc.id;\n            departments.push(appData);\n            });\n        this.departments = departments;\n        },\n        \n        async addDepartment(){\n        if(this.name && this.location && this.phonenmbr && this.headattendant) {\n          await db.collection(\"afdelingDB\").add({\n            name: this.name,\n            location: this.location,\n            phonenmbr: this.phonenmbr,\n            headattendant: this.headattendant\n          });\n          this.getDepartment();\n          this.name = \"\";\n          this.location = \"\";\n          this.phonenmbr = \"\";\n          this.headattendant  = \"\";\n          this.dialog = false;\n          alert(\"Afdeling is toegevoegd\")\n        } else{\n          alert('Naam, locatie, telefoonnummer en hoofdbegeleider zijn verplicht')\n        }\n      },\n        close() {\n            if (confirm(\"Weet u zeker dat u wilt afsluiten?\")) {\n                this.dialog = false;\n            } else {\n                this.dialog = true;\n            }\n        }\n    }\n  }\n</script>"]}]}