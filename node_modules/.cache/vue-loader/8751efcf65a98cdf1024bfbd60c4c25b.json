{"remainingRequest":"/Users/daviddumas/Project-Ontime/project-ontime/node_modules/vue-loader/lib/index.js??vue-loader-options!/Users/daviddumas/Project-Ontime/project-ontime/src/components/FormAddClient.vue?vue&type=script&lang=js&","dependencies":[{"path":"/Users/daviddumas/Project-Ontime/project-ontime/src/components/FormAddClient.vue","mtime":1639050185114},{"path":"/Users/daviddumas/Project-Ontime/project-ontime/node_modules/cache-loader/dist/cjs.js","mtime":499162500000},{"path":"/Users/daviddumas/Project-Ontime/project-ontime/node_modules/babel-loader/lib/index.js","mtime":315532800000},{"path":"/Users/daviddumas/Project-Ontime/project-ontime/node_modules/cache-loader/dist/cjs.js","mtime":499162500000},{"path":"/Users/daviddumas/Project-Ontime/project-ontime/node_modules/vue-loader/lib/index.js","mtime":499162500000}],"contextDependencies":[],"result":[{"type":"Buffer","data":"base64:"},{"version":3,"sources":["FormAddClient.vue"],"names":[],"mappings":";;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AAuGA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA","file":"FormAddClient.vue","sourceRoot":"src/components","sourcesContent":["<template>\n    <v-dialog\n    v-model=\"dialog\"\n      persistent\n      max-width=\"600px\"\n    >\n        <template v-slot:activator=\"{ on, attrs }\">\n            <v-btn\n            color=\"#006027\"\n            dark\n            class=\"mx-2\"\n            fab\n            small\n            v-bind=\"attrs\"\n            v-on=\"on\"\n            >\n            <v-icon dark>mdi-account-plus</v-icon>\n            </v-btn>\n        </template>\n        <v-card>\n            <v-card-title>\n            <span class=\"text-h5\">Client Toevoegen</span>\n            </v-card-title>\n            <v-card-text>\n                <v-form @submit.prevent=\"addClient\">\n                    <v-container>\n                        <v-row>\n                            <v-col\n                                cols=\"12\"\n                                sm=\"6\"\n                                md=\"4\"\n                            >\n                                <v-text-field\n                                label=\"Voornaam*\"\n                                v-model=\"firstname\"\n                                prepend-icon=\"mdi-account\"\n                                required\n                                ></v-text-field>\n                            </v-col>\n\n                            <v-col\n                                cols=\"12\"\n                                sm=\"6\"\n                                md=\"4\"\n                            >\n                                <v-text-field\n                                label=\"Achternaam*\"\n                                v-model=\"lastname\"\n                                required\n                                ></v-text-field>\n                            </v-col>\n\n                            <v-col cols=\"12\">\n                                <v-text-field\n                                label=\"Email*\"\n                                v-model=\"email\"\n                                prepend-icon=\"mdi-email\"\n                                required\n                                ></v-text-field>\n                            </v-col>\n\n                            <v-col cols=\"12\">\n                                <v-text-field\n                                label=\"Telefoonnummer\"\n                                v-model=\"phonenmbr\"\n                                prepend-icon=\"mdi-phone\"\n                                ></v-text-field>\n                            </v-col>\n\n                            <v-col cols=\"12\">\n                                <v-text-field\n                                label=\"Zorgpasnummer*\"\n                                v-model=\"cardnmbr\"\n                                prepend-icon=\"mdi-card-account-details\"\n                                required\n                                ></v-text-field>\n                            </v-col>\n                        </v-row>\n                    </v-container>\n                    <small>*indicates required field</small>\n                    <v-btn\n                        color=\"red\"\n                        text\n                        @click=\"close\"\n                    >\n                        <v-icon>mdi-delete</v-icon>\n                        Close\n                    </v-btn>\n                    <v-btn\n                        color=\"green\"\n                        text\n                        type=\"submit\"\n                    >\n                        <v-icon>mdi-content-save</v-icon>\n                        Opslaan\n                    </v-btn>\n                </v-form>\n            </v-card-text>\n        </v-card>\n    </v-dialog>\n</template>\n\n<script>\nimport { db } from '@/main';\n  export default {\n    data() {\n        return {\n            dialog: false,\n            firstname: '',\n            lastname: '',\n            email: '',\n            phonenmbr: '',\n            cardnmbr: '',\n        }\n    },\n    mounted() {\n        this.getClient();\n    },\n    methods: {\n        async getClient(){\n            let snapshot = await db.collection(\"cliëntenDB\").get();\n            let clienten = [];\n            snapshot.forEach(doc => {\n            let appData = doc.data();\n            appData.id = doc.id;\n            clienten.push(appData);\n            });\n        this.clienten = clienten;\n        },\n        \n        async addClient(){\n        if(this.firstname && this.lastname && this.email) {\n          await db.collection(\"cliëntenDB\").add({\n            firstname: this.firstname,\n            lastname: this.lastname,\n            email: this.email,\n            phonenmbr: this.phonenmbr,\n            cardnmbr: this.cardnmbr,\n          });\n          this.getClient();\n          this.firstname = \"\";\n          this.lastname = \"\";\n          this.email = \"\";\n          this.phonenmbr = \"\";\n          this.cardnmbr  = \"\";\n          this.dialog = false;\n          alert(\"Cliënt is toegevoegd\")\n        } else{\n          alert('Voornaam, achternaam en email zijn verplicht')\n        }\n      },\n        close() {\n            if (confirm(\"Weet u zeker dat u wilt afsluiten?\")) {\n                this.dialog = false;\n            } else {\n                this.dialog = true;\n            }\n        }\n    }\n  }\n</script>"]}]}