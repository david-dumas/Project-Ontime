A,IAAA,YAZA,0BAYA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,sBACA,MAAA,CAAA,SAAA,IAAA,MAAA,CAAA,QAAA,IAAA,MAAA,CAAA,KAAA,IAAA,MAAA,CAAA,QADA;AAAA;AAAA;AAAA;;AAAA;AAAA,uBAEA,EAAA,CAAA,UAAA,CAAA,cAAA,EAAA,GAAA,CAAA;AACA,kBAAA,SAAA,EAAA,MAAA,CAAA,SADA;AAEA,kBAAA,QAAA,EAAA,MAAA,CAAA,QAFA;AAGA,kBAAA,KAAA,EAAA,MAAA,CAAA,KAHA;AAIA,kBAAA,SAAA,EAAA,MAAA,CAAA,SAJA;AAKA,kBAAA,QAAA,EAAA,MAAA,CAAA;AALA,iBAAA,CAFA;;AAAA;AASA,gBAAA,MAAA,CAAA,YAAA;;AACA,gBAAA,MAAA,CAAA,SAAA,GAAA,EAAA;AACA,gBAAA,MAAA,CAAA,QAAA,GAAA,EAAA;AACA,gBAAA,MAAA,CAAA,KAAA,GAAA,EAAA;AACA,gBAAA,MAAA,CAAA,SAAA,GAAA,EAAA;AACA,gBAAA,MAAA,CAAA,QAAA,GAAA,EAAA;AACA,gBAAA,MAAA,CAAA,MAAA,GAAA,KAAA;AACA,gBAAA,KAAA,CAAA,0BAAA,CAAA;AAhBA;AAAA;;AAAA;AAkBA,gBAAA,KAAA,CAAA,0DAAA,CAAA;;AAlBA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAoBA,KAhCA;AAiCA,IAAA,KAjCA,mBAiCA;AACA,UAAA,OAAA,CAAA,oCAAA,CAAA,EAAA;AACA,aAAA,MAAA,GAAA,KAAA;AACA,OAFA,MAEA;AACA,aAAA,MAAA,GAAA,IAAA;AACA;AACA;AAvCA;AAdA,CAAA","sourcesContent":["<template>\n    <v-dialog\n    v-model=\"dialog\"\n      persistent\n      max-width=\"600px\"\n    >\n        <template v-slot:activator=\"{ on, attrs }\">\n            <v-btn\n            color=\"#006027\"\n            dark\n            class=\"mx-2\"\n            fab\n            small\n            v-bind=\"attrs\"\n            v-on=\"on\"\n            >\n            <v-icon dark>mdi-account-plus</v-icon>\n            </v-btn>\n        </template>\n        <v-card>\n            <v-card-title>\n            <span class=\"text-h5\">Begeleider Toevoegen</span>\n            </v-card-title>\n            <v-card-text>\n                <v-form @submit.prevent=\"addAttendant\">\n                    <v-container>\n                        <v-row>\n                            <v-col\n                                cols=\"12\"\n                                sm=\"6\"\n                                md=\"4\"\n                            >\n                                <v-text-field\n                                label=\"Voornaam*\"\n                                v-model=\"firstname\"\n                                prepend-icon=\"mdi-account\"\n                                required\n                                ></v-text-field>\n                            </v-col>\n\n                            <v-col\n                                cols=\"12\"\n                                sm=\"6\"\n                                md=\"4\"\n                            >\n                                <v-text-field\n                                label=\"Achternaam*\"\n                                v-model=\"lastname\"\n                                required\n                                ></v-text-field>\n                            </v-col>\n\n                            <v-col cols=\"12\">\n                                <v-text-field\n                                label=\"Email*\"\n                                v-model=\"email\"\n                                prepend-icon=\"mdi-email\"\n                                required\n                                ></v-text-field>\n                            </v-col>\n\n                            <v-col cols=\"12\">\n                                <v-text-field\n                                label=\"Telefoonnummer\"\n                                v-model=\"phonenmbr\"\n                                prepend-icon=\"mdi-phone\"\n                                ></v-text-field>\n                            </v-col>\n\n                            <v-col cols=\"12\">\n                                <v-text-field\n                                label=\"Password*\"\n                                type=\"password\"\n                                v-model=\"password\"\n                                prepend-icon=\"mdi-lock\"\n                                required\n                                ></v-text-field>\n                            </v-col>\n                        </v-row>\n                    </v-container>\n                    <small>*indicates required field</small>\n                    <v-btn\n                        color=\"red\"\n                        text\n                        left\n                        class=\"left\"\n                        @click=\"close\"\n                    >\n                        <v-icon>mdi-delete</v-icon>\n                        Close\n                    </v-btn>\n                    <v-btn\n                        color=\"green\"\n                        text\n                        right\n                        class=\"left\"\n                        type=\"submit\"\n                    >\n                        <v-icon>mdi-content-save</v-icon>\n                        Opslaan\n                    </v-btn>\n                </v-form>\n            </v-card-text>\n        </v-card>\n    </v-dialog>\n</template>\n\n<script>\nimport { db } from '@/main';\n  export default {\n    data() {\n        return {\n            dialog: false,\n            firstname: '',\n            lastname: '',\n            email: '',\n            phonenmbr: '',\n            password: '',\n        }\n    },\n    mounted() {\n        this.getAttendant();\n    },\n    methods: {\n        async getAttendant(){\n            let snapshot = await db.collection(\"begeleiderDB\").get();\n            let attendants = [];\n            snapshot.forEach(doc => {\n            let appData = doc.data();\n            appData.id = doc.id;\n            attendants.push(appData);\n            });\n        this.attendants = attendants;\n        },\n        \n        async addAttendant(){\n        if(this.firstname && this.lastname && this.email && this.password) {\n          await db.collection(\"begeleiderDB\").add({\n            firstname: this.firstname,\n            lastname: this.lastname,\n            email: this.email,\n            phonenmbr: this.phonenmbr,\n            password: this.password,\n          });\n          this.getAttendant();\n          this.firstname = \"\";\n          this.lastname = \"\";\n          this.email = \"\";\n          this.phonenmbr = \"\";\n          this.password  = \"\";\n          this.dialog = false;\n          alert(\"Begeleider is toegevoegd\")\n        } else{\n          alert('Voornaam, achternaam, email en wachtwoord zijn verplicht')\n        }\n      },\n        close() {\n            if (confirm(\"Weet u zeker dat u wilt afsluiten?\")) {\n                this.dialog = false;\n            } else {\n                this.dialog = true;\n            }\n        }\n    }\n  }\n</script>"],"sourceRoot":"src/components"}]}