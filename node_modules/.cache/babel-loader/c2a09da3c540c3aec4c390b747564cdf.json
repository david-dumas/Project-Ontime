AAA;AAAA;AAAA,sBACA,MAAA,CAAA,IAAA,IAAA,MAAA,CAAA,QAAA,IAAA,MAAA,CAAA,SAAA,IAAA,MAAA,CAAA,aADA;AAAA;AAAA;AAAA;;AAAA;AAAA,uBAEA,EAAA,CAAA,UAAA,CAAA,YAAA,EAAA,GAAA,CAAA;AACA,kBAAA,IAAA,EAAA,MAAA,CAAA,IADA;AAEA,kBAAA,QAAA,EAAA,MAAA,CAAA,QAFA;AAGA,kBAAA,SAAA,EAAA,MAAA,CAAA,SAHA;AAIA,kBAAA,aAAA,EAAA,MAAA,CAAA;AAJA,iBAAA,CAFA;;AAAA;AAQA,gBAAA,MAAA,CAAA,aAAA;;AACA,gBAAA,MAAA,CAAA,IAAA,GAAA,EAAA;AACA,gBAAA,MAAA,CAAA,QAAA,GAAA,EAAA;AACA,gBAAA,MAAA,CAAA,SAAA,GAAA,EAAA;AACA,gBAAA,MAAA,CAAA,aAAA,GAAA,EAAA;AACA,gBAAA,MAAA,CAAA,MAAA,GAAA,KAAA;AACA,gBAAA,KAAA,CAAA,wBAAA,CAAA;AAdA;AAAA;;AAAA;AAgBA,gBAAA,KAAA,CAAA,iEAAA,CAAA;;AAhBA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAkBA,KA9BA;AA+BA,IAAA,KA/BA,mBA+BA;AACA,UAAA,OAAA,CAAA,oCAAA,CAAA,EAAA;AACA,aAAA,MAAA,GAAA,KAAA;AACA,OAFA,MAEA;AACA,aAAA,MAAA,GAAA,IAAA;AACA;AACA;AArCA;AAbA,CAAA","sourcesContent":["<template>\n    <v-dialog\n    v-model=\"dialog\"\n      persistent\n      max-width=\"600px\"\n    >\n        <template v-slot:activator=\"{ on, attrs }\">\n            <v-btn\n            color=\"#006027\"\n            dark\n            class=\"mx-2\"\n            fab\n            small\n            v-bind=\"attrs\"\n            v-on=\"on\"\n            >\n            <v-icon dark>mdi-plus</v-icon>\n            </v-btn>\n        </template>\n        <v-card>\n            <v-card-title>\n            <span class=\"text-h5\">Afdeling Toevoegen</span>\n            </v-card-title>\n            <v-card-text>\n                <v-form @submit.prevent=\"addDepartment\">\n                    <v-container>\n                        <v-row>\n                            <v-col\n                                cols=\"12\"\n                            >\n                                <v-text-field\n                                label=\"Afdelingnaam*\"\n                                v-model=\"name\"\n                                prepend-icon=\"mdi-office-building\"\n                                required\n                                ></v-text-field>\n                            </v-col>\n\n                            <v-col cols=\"12\">\n                                <v-text-field\n                                label=\"Locatie*\"\n                                v-model=\"location\"\n                                prepend-icon=\"mdi-map-marker\"\n                                required\n                                ></v-text-field>\n                            </v-col>\n\n                            <v-col cols=\"12\">\n                                <v-text-field\n                                label=\"Telefoonnummer*\"\n                                v-model=\"phonenmbr\"\n                                prepend-icon=\"mdi-phone\"\n                                ></v-text-field>\n                            </v-col>\n\n                            <v-col cols=\"12\">\n                                <v-text-field\n                                label=\"Hoofdbegeleider*\"\n                                v-model=\"headattendant\"\n                                prepend-icon=\"mdi-account\"\n                                required\n                                ></v-text-field>\n                            </v-col>\n                        </v-row>\n                    </v-container>\n                    <small>*indicates required field</small>\n                    <v-btn\n                        color=\"red\"\n                        text\n                        @click=\"close\"\n                    >\n                        <v-icon>mdi-delete</v-icon>\n                        Close\n                    </v-btn>\n                    <v-btn\n                        color=\"green\"\n                        text\n                        type=\"submit\"\n                    >\n                        <v-icon>mdi-content-save</v-icon>\n                        Opslaan\n                    </v-btn>\n                </v-form>\n            </v-card-text>\n        </v-card>\n    </v-dialog>\n</template>\n\n<script>\nimport { db } from '@/main';\n  export default {\n    data() {\n        return {\n            dialog: false,\n            name: '',\n            location: '',\n            phonenmbr: '',\n            headattendant: '',\n        }\n    },\n    mounted() {\n        this.getDepartment();\n    },\n    methods: {\n        async getDepartment(){\n            let snapshot = await db.collection(\"afdelingDB\").get();\n            let departments = [];\n            snapshot.forEach(doc => {\n            let appData = doc.data();\n            appData.id = doc.id;\n            departments.push(appData);\n            });\n        this.departments = departments;\n        },\n        \n        async addDepartment(){\n        if(this.name && this.location && this.phonenmbr && this.headattendant) {\n          await db.collection(\"afdelingDB\").add({\n            name: this.name,\n            location: this.location,\n            phonenmbr: this.phonenmbr,\n            headattendant: this.headattendant\n          });\n          this.getDepartment();\n          this.name = \"\";\n          this.location = \"\";\n          this.phonenmbr = \"\";\n          this.headattendant  = \"\";\n          this.dialog = false;\n          alert(\"Afdeling is toegevoegd\")\n        } else{\n          alert('Naam, locatie, telefoonnummer en hoofdbegeleider zijn verplicht')\n        }\n      },\n        close() {\n            if (confirm(\"Weet u zeker dat u wilt afsluiten?\")) {\n                this.dialog = false;\n            } else {\n                this.dialog = true;\n            }\n        }\n    }\n  }\n</script>"],"sourceRoot":"src/components"}]}